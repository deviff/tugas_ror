<% if flash[:error] %>
  <p class="alert-error"><%= flash[:error] %></p>
<% end %>
<h3>Import Data Mahasiswa By XLS</h3>
<%= form_for :dump, :url=>{:controller=>"mahasiswas",
:action=>"doimport_excel"}, :html => { :multipart => true } do |f| %>
Select an Excel File :
<%= f.file_field :excel_file %>
<br/>
<%= submit_tag 'Submit' %>
<% end %>