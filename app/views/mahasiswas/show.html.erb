<div id='wrapper'>
<h3 align="center">Data Detail <%= @mahasiswa.nama %></h3>
<div id='content'>
<table class="table table-hover" style="width:80%" align="center">
	<tr>
		<th>NIM:</th>
		<td><%= @mahasiswa.nim %></td>
	</tr>
	<tr>
		<th>Firstname:</th>
		<td><%= @mahasiswa.first_name %></td>
	</tr>
	<tr>
		<th>Lastname:</th>
		<td><%= @mahasiswa.last_name %></td>
	</tr>
	<tr>
		<th>Nama:</th>
		<td><%= @mahasiswa.nama %></td>
	</tr>
	<tr>
		<th>Username:</th>
		<td><%= @mahasiswa.username %></td>
	</tr>
	<tr>
		<th>Semester:</th>
		<td><%= @mahasiswa.semester %></td>
	</tr>
	<tr>
		<th>Tingkat:</th>
		<td><%= @mahasiswa.ttingkat %></td>
	</tr>
	<tr>
		<th>Email:</th>
		<td><%= @mahasiswa.email %></td>
	</tr>
	<tr>
		<th>Phone:</th>
		<td><%= @mahasiswa.phone %></td>
	</tr>
	<tr>
		<th>Alamat:</th>
		<td><%= @mahasiswa.alamat %></td>
	</tr>
	<tr>
		<th>Tempat Tanggal Lahir:</th>
		<td><%=  @mahasiswa.tmptlahir %>,<%= @mahasiswa.tgllahir %></td>
	</tr>
	
</table>

</div>

